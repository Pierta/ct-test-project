{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "OpenTelemetry": {
      "LogLevel": {
        "Default": "Information",
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning"
      },
      "IncludeFormattedMessage": false,
      "IncludeScopes": true,
      "ParseStateValues": false,
      "IncludeAttributes": false,
      "IncludeTraceState": false
    }
  },
  "AllowedHosts": "*"
}
